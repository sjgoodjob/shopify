				{% if field == "form" %}<p class="sc-gLDmcm egKBBk">Sorry, the submission can not be sent! Please try again later!</p>{{ form.errors.messages[field] }}
				{% endif %}
				{% if form.errors.messages[field] contains "can't be blank." %}<p class="sc-gLDmcm egKBBk">Field can not be blank</p>{% else %}<p class="sc-gLDmcm egKBBk">Sorry, the submission can not be sent! Please try again later!</p>{% endif %}
